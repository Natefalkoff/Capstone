
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<h1 class="image">
    <a href="@Url.Action("Index","Home")"><img  style="height:300px; width:700px; margin:auto; position:center; padding-top: 52px"  src="~/Img/Cancer-Support-Community.jpg" alt="Cancer Support Community Central Ohio" /></a>
</h1>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>_Layout</title>

    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

</head>
 
<body>


    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container, nav">
            <div>
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                @Html.ActionLink("Home |", "Index", "Home", new { area=""}, new { @class = "navbar-brand"})
                @Html.ActionLink(" Register |", "Register", "User", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink(" Login |", "Login", "User", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink(" Add Recipe |", "AddRecipe", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink(" Search |", "Search", "Search", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink(" Plans |", "Index", "Plan", new { area = "" }, new { @class = "navbar-brand" })

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
            </div>
        </div>
    </div>


    <div>
        @RenderBody()
    </div>
</body>
</html>
